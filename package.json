{
  "name": "{{name}}",
  "version": "{{version}}",
  "author": "{{author}}",
  "scripts": {
    "start": "babel ./src/ -d ./lib/",
    "dev": "babel --watch ./src/ -d ./lib/",
    "prepublish": "npm start"
  },
  "vide": {
    "type": "prompt",
    "promptExtension": [
      {{promptExtension}}
    ],
    "promptPriority": 1
  },
  "keywords": [
    "vide-plugin",
    "{{keyword}}"
  ],
  "main": "lib/index.js",
  "dependencies": {
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-preset-es2015": "^6.18.0"
  }
}
